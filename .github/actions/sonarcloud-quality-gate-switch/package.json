    "axios": "^0.21.4",
    "chai": "^4.3.10"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.2",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@vercel/ncc": "^0.33.3",
    "dotenv": "^16.0.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29",
    "prettier": "^2.3.1",
    "ts-jest": "^29",
    "ts-node": "^10.7.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "./jest-preset.js"
